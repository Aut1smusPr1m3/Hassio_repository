{
  "name": "Emulated HUE Modded",
  "version": "latest",
  "description": "Hue Emulation for Home Assistant (custom build)",
  "url": "https://github.com/aut1smuspr1m3/emulatedhue_reworked",
  "slug": "emulated_hue_reworked",
  "stage": "stable",
  "startup": "application",
  "boot": "auto",
  "arch": ["amd64", "i386"],
  "map": ["config:rw"],
  "host_network": true,
  "environment": {
    "EMULATED_HUE_LABEL_FILTER": "light"
  },
  "homeassistant_api": true,
  "init": false,
  "options": {
    "verbose": false
  },
  "schema": {
    "http_port": "int?",
    "https_port": "int?",
    "use_default_ports_for_discovery": "bool?",
    "verbose": "bool"
  },
  "image": "ghcr.io/aut1smuspr1m3/emulatedhue_reworked"
}
















